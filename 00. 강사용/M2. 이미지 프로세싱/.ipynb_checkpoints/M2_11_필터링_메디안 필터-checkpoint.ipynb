    "import cv2\n",
    "\n",
    "src = cv2.imread(\"../images/noise.bmp\", cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "dst = cv2.cv2.medianBlur(src, 3)  #필터 사이즈 3x3\n",
    "\n",
    "cv2.imshow(\"src\", src)\n",
    "cv2.imshow(\"dst\", dst)\n",
    "\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
