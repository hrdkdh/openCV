    "import cv2\n",
    "\n",
    "src = cv2.imread(\"../images/leaf.jpg\", cv2.IMREAD_GRAYSCALE)\n",
    "aa, t1=cv2.threshold(src, 0, 255, cv2.THRESH_BINARY | cv2.THRESH_OTSU)\n",
    "\n",
    "print (\"threshhold =\",aa)\n",
    "\n",
    "cv2.imshow(\"src\", src) \n",
    "cv2.imshow(\"t1\", t1)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()\n"
   ]
