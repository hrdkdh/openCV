    "import cv2\n",
    "\n",
    "src = cv2.imread(\"../images/house.jpg\")\n",
    "\n",
    "dst = cv2.Canny(src, 50, 150)   # threshold low 50, high 150 지정\n",
    "\n",
    "cv2.imshow(\"src\", src)\n",
    "cv2.imshow(\"dst\", dst)\n",
    "cv2.waitKey()\n",
    "\n",
    "cv2.destroyAllWindows()\n"
   ]
  }
